{
  "name": "vscode-auto-open-markdown-preview-enhanced",
  "displayName": "Auto-Open Markdown Preview Enhanced",
  "description": "Open Markdown preview enhanced automatically when opening a Markdown file",
  "version": "0.0.5",
  "publisher": "timgreen",
  "repository": "https://github.com/timgreen/vscode-auto-open-markdown-preview-enhanced",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
